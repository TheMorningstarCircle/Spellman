local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Era of Encanto Made By ɹɐʇsɓuᴉuɹoW ɐuᴉɹqɐS", "Midnight")

-- Characters
local Tab = Window:NewTab("Characters")
local Section = Tab:NewSection("Characters")

Section:NewButton("Isabela", "", function()
    -- Script generated by Nut The Menace Spy - credits to Claudev0#8849

    local args = {
        [1] = "Isabela",
        [2] = "STOP_EXPLOITING"
    }

    game:GetService("ReplicatedStorage").Events.SetCharacter:FireServer(unpack(args))
end)

