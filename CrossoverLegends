local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Crossover Legends GUI Made By ɹɐʇsɓuᴉuɹoW ɐuᴉɹqɐS", "Ocean")

-- Characters
local Tab = Window:NewTab("Characters")
local Section = Tab:NewSection("Wanda")

Section:NewButton("Age of Ultron Wanda", "", function()
    local args = {
        [1] = "AOUWanda"
    }

    game:GetService("ReplicatedStorage").ChangePlayerCharacter:FireServer(unpack(args))

    workspace.Teleport10.RemoteFunction:InvokeServer()

end)

