local Section = Tab:NewSection("Maleficent Powers")

Section:NewToggle("Transfiguration", "Keybind (1)", function()

    local function getPlayer()
        local userInput = game:GetService("UserInputService")
        local cam = workspace.CurrentCamera
        local mousePos = userInput:GetMouseLocation()
        local mouseRay = cam:ViewportPointToRay(mousePos.X, mousePos.Y)
        players = {}
        for _, x in next, game:GetService('Players'):GetPlayers() do
            players[#players + 1] = x.Character
        end
        local raycastParams = RaycastParams.new()
        raycastParams.FilterType = Enum.RaycastFilterType.Whitelist
        raycastParams.FilterDescendantsInstances = players
        raycastParams.IgnoreWater = true
        local raycastResult = workspace:Raycast(mouseRay.Origin, mouseRay.Direction * 1000, raycastParams)
        if raycastResult then
            for _, x in next, game:GetService('Players'):GetPlayers() do
                if not raycastResult.Instance:IsDescendantOf(x.Character) then
                    continue
                end
                return x
            end
        end
        return false
    end

    local function Set(list)
        local set = {}
        for _, l in ipairs(list) do
            if l then
                set[l] = true
            end
        end
        return set
    end

    game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
        if key == "1" then
            local player = getPlayer()
            if player then

                local args = {
                    [1] = player.Character.RightLowerArm,
                    [2] = Vector3.new(-2434.853759765625, 1166.3641357421875, -346.0168151855469)
                }
                
                game:GetService("Players").LocalPlayer.Character.Transfiguration.Fire:FireServer(unpack(args))
         end
        end
    end)
end)

Section:NewButton("Transfiguration All", "", function()
    local Plr = game.Players.LocalPlayer
    for i, Player in pairs(game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 100000 then
                local Target = Player.Character or Player.CharacterAdded:Wait()

                local args = {
                    [1] = Target.RightLowerArm,
                    [2] = Vector3.new(-2434.853759765625, 1166.3641357421875, -346.0168151855469)
                }
                
                game:GetService("Players").LocalPlayer.Character.Transfiguration.Fire:FireServer(unpack(args))
            end
        end
    end

    local Plr = game.Players.LocalPlayer
    for i, Player in pairs(game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 10000 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
 
                
                local args = {
                    [1] = Target.RightLowerArm,
                    [2] = Vector3.new(-2434.853759765625, 1166.3641357421875, -346.0168151855469)
                }
                
                game:GetService("Players").LocalPlayer.Character.Transfiguration.Fire:FireServer(unpack(args))
            

            end
        end
    end

    local Plr = game.Players.LocalPlayer
    for i, Player in pairs(game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 1000 then
     
                local args = {
                    [1] = Target.RightLowerArm,
                    [2] = Vector3.new(-2434.853759765625, 1166.3641357421875, -346.0168151855469)
                }
                
                game:GetService("Players").LocalPlayer.Character.Transfiguration.Fire:FireServer(unpack(args))

            end
        end
    end



    local Plr = game.Players.LocalPlayer
    for i, Player in pairs(game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 100 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
        
                local args = {
                    [1] = Target.RightLowerArm,
                    [2] = Vector3.new(-2434.853759765625, 1166.3641357421875, -346.0168151855469)
