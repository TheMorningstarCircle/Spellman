local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("The Jedi Academy on Dantooine GUI Made By ɹɐʇsɓuᴉuɹoW ɐuᴉɹqɐS", "Sentinel")

local Tab = Window:NewTab("Team")
local Section = Tab:NewSection("Change Team")
local Section = Tab:NewSection("Join the group to Join Jedi Order")
Section:NewButton("Jedi Order", "", function()
    -- Script generated by Nut The Menace Spy - credits to Claudev0#8849

    local args = {
        [1] = "Jedi Order"
    }

    game:GetService("ReplicatedStorage").TeamSelection:FireServer(unpack(args))

end)

