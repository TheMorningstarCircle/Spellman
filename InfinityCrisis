Section:NewButton("Telekenesis All", "", function()

    -- Script generated by ClaudiaSpy - credits to ClaudiaHOTM#2452
     local Plr = game.Players.LocalPlayer
     for i, Player in pairs(game.Players:GetPlayers()) do
 
         if Player.Character ~= nil and Player ~= Plr then
             local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
             if mag < 10000 then
                 local Target = Player.Character or Player.CharacterAdded:Wait()
 
 
        -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Target",
    [2] = true,
    [3] = Target,
    [4] = game:GetService("Players").LocalPlayer.Character.PowerClient.Telekinesis.Debounce,
    [5] = game:GetService("Players").LocalPlayer.Character.PowerClient.Telekinesis.Active,
    [6] = game:GetService("Players").LocalPlayer.Character.PowerClient.Telekinesis.Object
}

game:GetService("ReplicatedStorage").Powers:FireServer(unpack(args))
 
             end
         end
     end
 
 end)

