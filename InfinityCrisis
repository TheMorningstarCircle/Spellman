local Library = loadstring(game:HttpGet(
                               "https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib(
                   "Infinity Crisis Made By ɹɐʇsɓuᴉuɹoW ɐuᴉɹqɐS",
                   "Sentinel")

local Tab = Window:NewTab("Characters")
local Section = Tab:NewSection("Main")
Section:NewButton("Destory Intro Gui", "", function()
    game.Players.LocalPlayer.PlayerGui.IntroGui:destroy()
end)

