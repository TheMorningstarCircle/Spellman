local Tab = Window:NewTab("Teleport")
local Section = Tab:NewSection("Places")

Section:NewButton("Spawn 1", "", function()
    game.Players.LocalPlayer.Character:MoveTo(Vector3.new(388, 14, -83))
end)

Section:NewButton("Spawn 2", "", function()
    game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-76, 13, -319))
end)

Section:NewButton("Spawn 3", "", function()
    game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-78, 16, 794))
end)

