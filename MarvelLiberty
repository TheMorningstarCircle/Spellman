local Tab = Window:NewTab("Powers")
local Section = Tab:NewSection("Agatha Harkness")

Section:NewTextBox("Reality Transfiguration Someone", "", function(txt)
    local Players = game:GetService("Players")
    local msg = txt
    local function findPlayer(stringg)
        for _, v in pairs(Players:GetPlayers()) do
            if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                return v
            end
        end
    end

    local player = findPlayer(msg)
    local CHAR = player.Character
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame

  -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "\0Agatha Harkness Reality Transfiguration\0\0",
    [2] = player.Character
}

game:GetService("ReplicatedStorage").Network.RemoteEvent:FireServer(unpack(args))

end)

Section:NewButton("Shield On", "", function()
  -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "\0Agatha Harkness Start Shield\0\0"
}

game:GetService("ReplicatedStorage").Network.RemoteEvent:FireServer(unpack(args))
end)

Section:NewButton("Shield Off", "", function()
    -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "\0Agatha Harkness Stop Shield\0\0"
}

game:GetService("ReplicatedStorage").Network.RemoteEvent:FireServer(unpack(args))
end)

