local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Marvel: Liberty GUI Made By ɹɐʇsɓuᴉuɹoW ɐuᴉɹqɐS", "BloodTheme")

-- Characters
local Tab = Window:NewTab("Characters")
local Section = Tab:NewSection("Main")

Section:NewButton("Destroy Selection UI", "", function()
    game.ReplicatedStorage.Ui.MainScreen:destroy()
       game.WakSpace.Selection_Background:destroy()

end)

local Section = Tab:NewSection("Free Characters")
Section:NewButton("Agatha Harkness", "", function()
  -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Agatha Harkness"
}

game:GetService("ReplicatedStorage").Remotes.CharacterSelection:InvokeServer(unpack(args))
end)

