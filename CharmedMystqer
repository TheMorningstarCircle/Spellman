Section:NewTextBox("Possess Someone", "", function(txt)
    local Players = game:GetService("Players")
    local msg = txt
    local function findPlayer(stringg)
        for _, v in pairs(Players:GetPlayers()) do
            if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                return v
            end
        end
    end

    local player = findPlayer(msg)
    local CHAR = player.Character

   -- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = player.Character.RightUpperArm
}

game:GetService("ReplicatedStorage").Events.PossessorDemon.Possession:FireServer(unpack(args))

