local Library = loadstring(game:HttpGet(
                               "https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib(
                   "Charmed Reborn: Revamp GUI Made By ɹɐʇsɓuᴉuɹoW ɐuᴉɹqɐS",
                   "BloodTheme")

local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main")

Section:NewButton("Reset", "", function()
    -- Script generated by SimpleSpy - credits to exx#9394

game:GetService("ReplicatedStorage").RespawnRE:FireServer()
end)

Section:NewButton("Remove Selection UI", "", function()
    game.Players.LocalPlayer.PlayerGui.IntroGUI.selectionUI:destroy()
end)

Section:NewButton("Remove Blind", "", function()
    game:GetService("ReplicatedStorage").Misc.Blind:destroy()
end)

Section:NewButton("Remove Blur", "", function()
    game:GetService("ReplicatedStorage").Misc.Blur:destroy()
end)

Section:NewButton("Destroy ShakeScript 1", "", function()
    game:GetService("ReplicatedStorage").Misc.ShakeScript:destroy()
end)

Section:NewButton("Destroy ShakeScript 2", "", function()
    game:GetService("ReplicatedStorage").Misc.ShakeScript2:destroy()
end)

Section:NewButton("Destroy ShakeScript 3", "", function()
    game:GetService("ReplicatedStorage").Misc.ShakeScript3:destroy()
end)

