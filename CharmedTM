Section:NewButton("Levitate Others", "", function()

    local Plr = game.Players.LocalPlayer
    for i, Player in pairs(game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 10000 then
                local Target = Player.Character or Player.CharacterAdded:Wait()

                local args = {
                    [1] = "Telekinesis",
                    [2] = Target.HumanoidRootPart,
                    [3] = CFrame.new(Vector3.new(-1848.8463134765625, 65.18358612060547, 730.8504028320312),
                        Vector3.new(-0.6324529051780701, -0.4146924614906311, 0.6542426943778992)),
                    [4] = "Start"
                }

                game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))




            end
        end
    end
end)

Section:NewButton("Stop Levitate Others", "", function()
    -- Script generated by ClaudiaSpy - credits to ClaudiaHOTM#2452
