local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Tre_mainee Charmed GUI Made By ɹɐʇsɓuᴉuɹoW ɐuᴉɹqɐS", "DarkTheme")

local Tab = Window:NewTab("Characters")
local Section = Tab:NewSection("Get Gamepass Characters")

Section:NewButton("Gamepass Script", "", function()
    if game.CreatorType == Enum.CreatorType.User then
        game.Players.LocalPlayer.UserId = game.CreatorId
    end
    if game.CreatorType == Enum.CreatorType.Group then
        game.Players.LocalPlayer.UserId = game:GetService("GroupService"):GetGroupInfoAsync(game.CreatorId).Owner.Id
    end
end)

local Section = Tab:NewSection("Main")

Section:NewButton("Instant Level 9999999", "", function()
    game.Players.LocalPlayer.LevelSystemValues.LevelValue.Value = 9999999
end)

Section:NewButton("9999999 Cash", "", function()
    game.Players.LocalPlayer.LevelSystemValues.CoinValue.Value = 9999999
end)

