Section:NewButton("Psychic Reflection All", "", function()

    local Plr = game.Players.LocalPlayer
    for i, Player in pairs(game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 10000 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
                -- Script generated by Nut The Menace Spy - credits to Claudev0#8849

                local args = {
                    [1] = "Psychic Reflection",
                    [2] = "Start",
                    [3] = Target.HumanoidRootPart
                }

                game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))

            end
        end
    end
end)

Section:NewButton("Psychic Reflection All End", "", function()

    local Plr = game.Players.LocalPlayer
    for i, Player in pairs(game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 10000 then
                local Target = Player.Character or Player.CharacterAdded:Wait()
                -- Script generated by Nut The Menace Spy - credits to Claudev0#8849

                local args = {
                    [1] = "Psychic Reflection",
                    [2] = "End",
                    [3] = Target.HumanoidRootPart
                }

                game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))

            end
        end
    end
end)

Section:NewTextBox("Possession", "", function(txt)
    local Players = game:GetService("Players")
    local msg = txt
    local function findPlayer(stringg)
        for _, v in pairs(Players:GetPlayers()) do
            if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                return v
            end
        end
    end

    local player = findPlayer(msg)
    local CHAR = player.Character
    -- Script generated by SimpleSpy - credits to exx#9394

    local args = {
        [1] = "Possession",
        [2] = player.Character.HumanoidRootPart,
        [3] = "Start"
    }

    game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))

end)

Section:NewTextBox("Possession End", "", function(txt)
    local Players = game:GetService("Players")
    local msg = txt
    local function findPlayer(stringg)
        for _, v in pairs(Players:GetPlayers()) do
            if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                return v
            end
        end
    end

    local player = findPlayer(msg)
    local CHAR = player.Character
    -- Script generated by SimpleSpy - credits to exx#9394

    local args = {
        [1] = "Possession",
        [2] = player.Character.HumanoidRootPart,
        [3] = "End"
    }

    game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))

end)

